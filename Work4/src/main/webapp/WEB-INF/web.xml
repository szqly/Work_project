        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.Servlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadf</servlet-name>
        <url-pattern>/uploadf</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>uploadf</servlet-name>
        <servlet-class>com.Servlet.uploadf</servlet-class>
    </servlet>
    <context-param>
        <param-name>encoding</param-name>
        <param-value>GBK</param-value>
    </context-param>
</web-app>